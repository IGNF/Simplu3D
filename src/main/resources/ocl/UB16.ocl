package  fr::ign::cogit::simplu3d::model::application



context SpecificCadastralBoundary
inv: (self.type=0 or self.type= 1) implies Building.allInstances().getFootprint().distance(self.geom)->min()>6


context Building
inv: self.height(1,2) < 12






context BasicPropertyUnit
inv: self.getBuildings().forAll(b1, b2 | b1 <> b2 implies b1.distance(b2) < 3)




endpackage
